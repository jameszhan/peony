[client] 
character-set-server=<%= mysql.charset %>
port=<%= mysql.port %>
socket=<%= mysql.socket %>
 
 
[mysqld] 
character-set-server=<%= mysql.charset %>
user=<%= user %>
port=<%= mysql.port %>
socket=<%= mysql.socket %>
basedir=<%= mysql.home %>
datadir=<%= data_dir %>

pid-file=<%= mysql.pid_file %>
log-error=<%= mysql.log_dir %>/error.log

slow_query_log=<%= mysql.slow_query_log %>
slow_query_log_file=<%= mysql.log_dir %>/slow-query.log

general_log=<%= mysql.general_log %>
general_log_file=<%= mysql.log_dir %>/general.log

 
relay-log-index=<%= mysql.log_dir %>/relay-index.log
relay-log-info-file=<%= mysql.log_dir %>/relay-info.log
relay-log=<%= mysql.log_dir %>/relay.log

explicit_defaults_for_timestamp=TRUE


[mysqldump] 
quick 
max_allowed_packet=24M
